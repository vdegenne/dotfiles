<dom-module id="${1:$(un-camelcase-string yas/text)}">
  <template>
  
  </template>

  <script>
    class ${1:className} extends Polymer.Element
    {
      static get is () { return '${1:$(un-camelcase-string yas/text)}'; }

      static get properties () {
        return {
          $2
        }
      }

      $0
    }

    customElements.define($1.is, $1);
  </script>
</dom-module>